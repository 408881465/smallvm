	spec ' ' '[net:stopWiFi]'				'stop WiFi'
	spec 'r' '[net:wifiStatus]'				'WiFi status'
	spec 'r' '[net:myIPAddress]'			'my IP address'
	spec ' ' '[net:httpRequest]'			'_ request http‹É// _ / _ : body _' 'menu.requestTypes str str str' 'GET' 'microblocks.fun' 'example.txt' ''
	spec 'r' '[net:httpChunkAvailable]'		'HTTP chunk available?'
	spec 'r' '[net:nextHttpChunk]'			'next HTTP chunk'
