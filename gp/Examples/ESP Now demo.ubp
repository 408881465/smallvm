module main
author unknown
version 1 0 
description ''
variables event 

script 122 40 {
forever {
  event = (callCustomReporter '[net:EspNowLastEvent]')
  if (event != (booleanConstant false)) {
    sayIt (at 1 event)
  }
}
}

script 40 314 {
whenStarted
forever {
  setUserLED false
  waitMillis 500
  local 'message' ('[data:join]' 'micro' (random 1 100))
  callCustomCommand '[net:EspNowSendMessage]' ('[data:makeList]' message)
  sayIt message
  setUserLED true
  waitMillis 500
}
}

